{
  "name": "mangabookmark-backend",
  "version": "4.4.1",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node bin/www.js",
    "dev": "docker compose up -d && nodemon bin/www.js",
    "down": "docker compose down",
    "lint": "eslint --fix .",
    "test": "NODE_ENV=test jest --runInBand",
    "coverage": "NODE_ENV=test jest --runInBand --coverage=true",
    "gendoc": "swagger-jsdoc -d swagger-def.cjs api/**/*.js models/**/*.js"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 10000,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/",
      "/services/manga-service/parsers"
    ],
    "coverageReporters": [
      "text",
      "html"
    ]
  },
  "dependencies": {
    "@awaitjs/express": "^0.9.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@googleapis/drive": "^4.0.1",
    "amqplib": "^0.10.3",
    "async-redis": "^2.0.0",
    "bcrypt": "^5.0.0",
    "cheerio": "^1.0.0-rc.5",
    "cloudinary": "^1.27.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.17.1",
    "express-enforces-ssl": "^1.1.0",
    "express-static-gzip": "^2.1.1",
    "express-validator": "^6.9.2",
    "got": "^12.5.3",
    "helmet": "^6.0.1",
    "jsbi": "^4.3.0",
    "lodash": "^4.17.20",
    "mongoose": "^6.9.0",
    "mongoose-fuzzy-searching": "^2.0.2",
    "multer": "^1.4.5-lts.1",
    "passport": "0.5",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "swagger-ui-express": "^4.1.4",
    "tunnel": "^0.0.6",
    "web-push": "^3.4.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@jest/environment": "^29.4.1",
    "@jest/globals": "^29.4.1",
    "@types/express": "^4.17.8",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "faker": "^6.6.6",
    "jest": "^29.4.1",
    "node-mongodb-fixtures": "^3.2.7",
    "nodemon": "^2.0.6",
    "prettier": "^2.0.5",
    "redis-commander": "^0.8.0",
    "supertest": "^6.1.6",
    "swagger-jsdoc": "^6.2.8"
  }
}
